{
  "name": "@rplan/password-strength",
  "version": "0.3.0",
  "description": "This package provides a set of rules for passwords.",
  "module": "lib/index.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "allex-ts-source": "src/index.ts",
  "repository": "https://github.com/actano/password-strength.git",
  "author": "Actano GmbH (https://rplan.com/)",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.19.3",
    "@babel/core": "^7.20.5",
    "@babel/node": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/register": "^7.18.9",
    "@rplan/allex-shared-config": "^2.0.0",
    "chai": "^4.3.7",
    "mocha": "^6.2.3",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "allex-build build",
